{
  "expo": {
    "name": "mobileProject",
    "slug": "mobileProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.mobileProject.findplace",
        "googleMapsApiKey": "AIzaSyCyOu-XbRZIYwaJpsCUn4lJgJAP-LdU0MM"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow JUSTIN to use your location."
          }
        ]
      ]
    },
    "plugins": [
      "expo-font"
    ]
  }
}
